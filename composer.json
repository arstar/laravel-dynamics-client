{
    "name": "justbetter/laravel-dynamics-client",
    "description": "A client to connect with Microsoft Dynamics",
    "type": "package",
    "authors": [
        {
            "name": "Vincent Boon",
            "email": "vincent@justbetter.nl",
            "role": "Developer"
        },
        {
            "name": "Ramon Rietdijk",
            "email": "ramon@justbetter.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "justbetter/odata-client": "^1.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "JustBetter\\DynamicsClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustBetter\\DynamicsClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustBetter\\DynamicsClient\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
